<?xml version="1.0" encoding="utf-8"?>
<library xmlns="duell">

    <supported-duell-tool version="4.0.0+" />

    <supported-build-plugin name="android" version="6.0.0+" />

    <platform-config>

        <android>
            <java-source name="APP_DELEGATE" path="java" />
            <proguard path="proguard.cfg" />

            <activity-extension name="org.haxe.duell.appdelegate.AppDelegate" />
        </android>

        <documentation>
            <documentation-folder library="android_appdelegate" path="documentation" />
        </documentation>

    </platform-config>

</library>
